URxvt*scrollColor: #777777
URxvt*scrollStyle: plain
URxvt*background:  #212121
URxvt*foreground:  white
URxvt*scrollBar:   false

Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb
Xft.dpi: 96
URxvt.font: xft:FuraCode Nerd Font:size=9,xft:Fura Mono Powerline:size=9
URxvt.letterSpace: -1
URxvt.internalBorder: 8

URxvt.transparent: true
URxvt.shading: 20

! Rofi Config
rofi.font: San Fransisco Display 18
rofi.color-normal: #00212121,white,#00212121,#D9212121,white
rofi.color-window: #D9212121,#D9212121
! rofi.fake-transparency: true
rofi.bw: 0
rofi.line-padding: 8
rofi.line-margin: 0
rofi.separator-style: none
rofi.modi: window,run,ssh
rofi.fullscreen: true
rofi.padding:400
! rofi.eh:2 
